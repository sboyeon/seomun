        <section class="table">
            <article>
                <div class="page">
                    <div>
                        <p>Total <span>87건</span> 1페이지</p>
                    </div>
                </div>
                <input type="button" value="게시판" onclick="javascript:location.href='/notice_write'">
            </article>
            <table>
                <thead>
                    <tr class="tbNotice">
                        <th class="num">번호</th>
                        <th class="subject">제목</th>
                        <th class="name">글쓴이</th>
                        <th class="date dis_none">날짜</th>
                        <th class="del dis_none">관리</th>
                    </tr>
                </thead>
                <tbody>
                <% rows.forEach((row, index) => { %>
                    <tr class="tb_notice">
                        <th class="num"><%= row.RNUM%></th>
                        <th class="subject"><a onclick="javascript:location.href='/notice_read?id=<%=row.id%>'"><%= row.title%></a></th>
                        <td class="name"><%= row.writer%></td>
                        <td class="date dis_none"><%= row.DATE%></td>
                        <td class="look de_none dis_none"><%= row.view%></td>
                        <td>
                            <input type="button" onclick="javascript:location.href='/updateBoard?id=<%=row.id%>'" value="수정">
                            <input type="button" onclick="javascript:location.href='/deleteBoard?id=<%=row.id%>'" value="삭제">
                        </td>
                    </tr>
                <% }) %>
                </tbody>
            </table>
        </section>
