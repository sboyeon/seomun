        <section class="notice_write">
            <h2>공지사항 수정</h2>
            <form method="POST" action="/store">
                <table class="noWrite_tb">
                    <tr>
                        <td class="noWrite_td"><label>제목 : </label></td>
                        <td class="noWrite_ipt"><input type="text" id="nt_title" name="title"></td>
                    </tr>
                    <tr>
                        <td class="noWrite_td"><label>이름 : </label></td>
                        <td class="noWrite_ipt"><input type="text" id="nt_name" name="writer"></td>
                    </tr>
                    <tr>
                        <td class="noWrite_td"><label>비밀번호 : </label></td>
                        <td class="noWrite_ipt"><input type="password" id="nt_password" name="password"></td>
                    </tr>
                    <tr>
                        <td class="noWrite_td">내용 작성 : </td>
                        <td class="noWrite_ipt"><textarea id="nt_content" rows="4" cols="40" name="content"></textarea></td>
                    </tr>
                    <tr>
                        <td class="noWrite_td">첨부파일 : </td>
                        <td class="noWrite_ipt"><input type="button" value="파일첨부" id="nt_llist"></td>
                    </tr>
                </table>
                <div class="notice_write_inputs">
                    <input type="submit" value="submit">
                    <input type="button" value="back" onclick="javascript:location.href='/notice'">
                </div>
            </form>
        </section>